//Swap
#include <stdio.h>
int a,b;
void swap(int *a,int *b)
{
    int temp=*a;
    *a=*b;
    *b=temp;
}
int main()
{
    printf("Enter 2 no:\n");
    scanf("%d %d",&a,&b);
    swap(&a,&b);
    printf("The swapped numbers are a = %d and b = %d.",a,b);
    return 0;
}

//Address
#include <stdio.h>
int main() 
{
    int a[5]={1,2,3,4,5};
    for(int i=0;i<5;i++)
    {
        printf("Address of %d is %p.\n",a[i],&a[i]);
    }
    return 0;
}
