//Storing at beginning
#include <stdio.h>
#include <stdlib.h>
struct cntct
{
    long int num;
    struct cntct *link;
};
int main()
{
    struct cntct *head=NULL;
    struct cntct *c1=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c2=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c3=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c4=(struct cntct*)malloc(sizeof(struct cntct));
    c1->num=9865326598;
    c2->num=6598326598;
    c3->num=6598743197;
    c4->num=8956431526;
    c1->link=head;
    head=c1;
    c2->link=head;
    head=c2;
    c3->link=head;
    head=c3;
    c4->link=head;
    head=c4;
    printf("Contact:\n");
    while(head!=NULL)
    {
        printf("%ld-->",head->num);
        head=head->link;
    }
    printf("NULL");
    return 0;
}

//Storing at ending
#include <stdio.h>
#include <stdlib.h>
struct contact
{
    long int num;
    struct contact *next;
};
int main() 
{
    struct contact *head=NULL,*temp;
    struct contact *ct1=(struct contact*)malloc(sizeof(struct contact));
    struct contact *ct2=(struct contact*)malloc(sizeof(struct contact));
    struct contact *ct3=(struct contact*)malloc(sizeof(struct contact));
    ct1->num=8956235689;
    ct2->num=6895326598;
    ct3->num=9988745625;
    ct1->next=NULL;
    ct2->next=NULL;
    ct3->next=NULL;
    head=ct1;
    temp=head;
    while(temp->next!=NULL)
        temp=temp->next;
    temp->next=ct2;
    temp=head;
    while(temp->next!=NULL)
        temp=temp->next;
    temp->next=ct3;
    temp=head;
    while(temp!=NULL)
    {
        printf("%ld -> ",temp->num);
        temp=temp->next;
    }
    printf("NULL");
    free(ct1);
    free(ct2);
    free(ct3);
    return 0;
}
