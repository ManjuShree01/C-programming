//Storing at beginning
#include <stdio.h>
#include <stdlib.h>
struct cntct
{
    long int num;
    struct cntct *link;
};
int main()
{
    struct cntct *head=NULL;
    struct cntct *c1=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c2=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c3=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c4=(struct cntct*)malloc(sizeof(struct cntct));
    c1->num=9865326598;
    c2->num=6598326598;
    c3->num=6598743197;
    c4->num=8956431526;
    c1->link=head;
    head=c1;
    c2->link=head;
    head=c2;
    c3->link=head;
    head=c3;
    c4->link=head;
    head=c4;
    printf("Contact:\n");
    while(head!=NULL)
    {
        printf("%ld-->",head->num);
        head=head->link;
    }
    printf("NULL");
    return 0;
}

//Storing at ending
#include <stdio.h>
#include <stdlib.h>
struct contact
{
    long int num;
    struct contact *next;
};
int main() 
{
    struct contact *head=NULL,*temp;
    struct contact *ct1=(struct contact*)malloc(sizeof(struct contact));
    struct contact *ct2=(struct contact*)malloc(sizeof(struct contact));
    struct contact *ct3=(struct contact*)malloc(sizeof(struct contact));
    ct1->num=8956235689;
    ct2->num=6895326598;
    ct3->num=9988745625;
    ct1->next=NULL;
    ct2->next=NULL;
    ct3->next=NULL;
    head=ct1;
    temp=head;
    while(temp->next!=NULL)
        temp=temp->next;
    temp->next=ct2;
    temp=head;
    while(temp->next!=NULL)
        temp=temp->next;
    temp->next=ct3;
    temp=head;
    while(temp!=NULL)
    {
        printf("%ld -> ",temp->num);
        temp=temp->next;
    }
    printf("NULL");
    free(ct1);
    free(ct2);
    free(ct3);
    return 0;
}

//Beginning
#include <stdio.h>
#include <stdlib.h>
struct insertbeginning
{
    int num;
    struct insertbeginning *next;
};
int main()
{
    struct insertbeginning *head=NULL;
    struct insertbeginning *n1=(struct insertbeginning*)malloc(sizeof(struct insertbeginning));
    struct insertbeginning *n2=(struct insertbeginning*)malloc(sizeof(struct insertbeginning));
    struct insertbeginning *n3=(struct insertbeginning*)malloc(sizeof(struct insertbeginning));
    struct insertbeginning *n4=(struct insertbeginning*)malloc(sizeof(struct insertbeginning));
    n3->num=10;
    n2->num=20;
    n1->num=30;
    n1->next=head;
    head=n1;
    n2->next=head;
    head=n2;
    n3->next=head;
    head=n3;
    n4->num=5;
    n4->next=head;
    head=n4;
    while(head!=NULL)
    {
        printf("%d",head->num);
        if(head->next!=NULL)
        {
            printf("->");
        }
        head=head->next;
    }
    return 0;
}

//Inserting at middle
#include <stdio.h>
#include <stdlib.h>
struct contact
{
    long int number;
    struct contact *next;
};
int main() {
    struct contact *head,*temp,*newcontact;
    head=malloc(sizeof(struct contact));
    head->number=9988776655;
    head->next=malloc(sizeof(struct contact));
    head->next->number=9876543210;
    head->next->next=malloc(sizeof(struct contact));
    head->next->next->number=9787776757;
    head->next->next->next=NULL;
    //inserting at middle
    newcontact =malloc(sizeof(struct contact));
    newcontact->number=9888786858;
    temp=head;
    while(temp!=NULL && temp->number!=9876543210)
    temp=temp->next;
    if(temp!=NULL)
    newcontact->next=temp->next;
    temp->next=newcontact;
    temp=head;
    while(temp!=NULL)
    {
        printf("%ld-->",temp->number);
        temp=temp->next;
    }
    printf("Finish");
    return 0;
}

//Deleting
#include <stdio.h>
#include <stdlib.h>
struct contact
{
    long int number;
    struct contact *next;
};
int main() {
    struct contact *head,*temp,*newcontact;
    head=malloc(sizeof(struct contact));
    head->number=9988776655;
    head->next=malloc(sizeof(struct contact));
    head->next->number=9876543210;
    head->next->next=malloc(sizeof(struct contact));
    head->next->next->number=9787776757;
    head->next->next->next=NULL;
    //inserting at middle
    newcontact =malloc(sizeof(struct contact));
    newcontact->number=9888786858;
    temp=head;
    while(temp!=NULL && temp->number!=9876543210)
    temp=temp->next;
    if(temp!=NULL)
    newcontact->next=temp->next;
    temp->next=newcontact;
    temp=head;
    while(temp!=NULL)
    {
        printf("%ld-->",temp->number);
        temp=temp->next;
    }
    printf("Finish\n");
    long int delnum=9888786858;
    temp=head;
    while(temp!=NULL)
    {
        printf("%ld-->",temp->number);
        temp=temp->next;
    }
     printf("Finish");
    return 0;
}

//Ending
