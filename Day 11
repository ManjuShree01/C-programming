//Push
#include <stdio.h>
#define size 5
int array[size];
int top=-1;
void push(int value)
{
    if(top==size-1)
    {
        printf("No space left");
    }
    else
    {
        top++;
        array[top]=value;
        printf("The value added is %d.\n",value);
    }
}
int main() {
    push(10);
    push(20);
    push(30);
    push(40);
    push(50);
    push(60);
    return 0;
}

//Pop
#include <stdio.h>
#define size 5
int array[size];
int top=-1;
void push(int value)
{
    if(top==size-1)
    {
        printf("No space left");
    }
    else
    {
        top++;
        array[top]=value;
        printf("The value added is %d.\n",value);
    }
}
void pop()
{
    if(top==-1)
    {
        printf("Stack is empty.");
    }
    else
    {
        printf("Top value b4 popping is %d.\n",array[top]);
        top--;
        printf("Top value after popping is %d.\n",array[top]);
    }
}
int main() {
    push(10);
    push(20);
    push(30);
    pop();
    pop();
    pop();
    pop();
    return 0;
}

//Peak value
#include <stdio.h>
#define size 5
int array[size];
int top=-1;
void push(int value)
{
    if(top==size-1)
    {
        printf("No space left");
    }
    else
    {
        top++;
        array[top]=value;
        printf("The value added is %d.\n",value);
    }
}
void pop()
{
    if(top==-1)
    {
        printf("Stack is empty.");
    }
    else
    {
        printf("Top value b4 popping is %d.\n",array[top]);
        top--;
        printf("Top value after popping is %d.\n",array[top]);
    }
}
void peak()
{
    if(top==-1)
    {
        printf("Stack is empty.");
    }
    else
    {
        printf("Peak value is %d.\n",array[top]);
    }
}
int main() {
    push(10);
    push(20);
    push(30);
    peak();
    pop();
    peak();
    pop();
    return 0;
}

//Isempty
#include <stdio.h>
#define size 5
int array[size];
int top=-1;
void push(int value)
{
    if(top==size-1)
    {
        printf("No space left");
    }
    else
    {
        top++;
        array[top]=value;
        printf("The value added is %d.\n",value);
    }
}
void pop()
{
    if(top==-1)
    {
        printf("Stack is empty.");
    }
    else
    {
        printf("Top value b4 popping is %d.\n",array[top]);
        top--;
        printf("Top value after popping is %d.\n",array[top]);
    }
}
void peak()
{
    if(top==-1)
    {
        printf("Stack is empty.");
    }
    else
    {
        printf("Peak value is %d.\n",array[top]);
    }
}
void isempty()
{
    if(top==-1)
    {
        printf("Stack is empty.\n");
    }
    else
    {
        printf("Stack is not empty.\n");
    }
}
int main() {
    isempty();
    push(10);
    push(20);
    push(30);
    peak();
    pop();
    peak();
    pop();
    isempty();
    return 0;
}

//Size
#include <stdio.h>
#define MAX 5
int array[MAX];
int top=-1;
void push(int value)
{
    if(top==MAX-1)
    {
        printf("No space left");
    }
    else
    {
        top++;
        array[top]=value;
        printf("The value added is %d.\n",value);
    }
}
void pop()
{
    if(top==-1)
    {
        printf("Stack is empty.");
    }
    else
    {
        printf("Top value b4 popping is %d.\n",array[top]);
        top--;
        printf("Top value after popping is %d.\n",array[top]);
    }
}
void peak()
{
    if(top==-1)
    {
        printf("Stack is empty.");
    }
    else
    {
        printf("Peak value is %d.\n",array[top]);
    }
}
void isempty()
{
    if(top==-1)
    {
        printf("Stack is empty.\n");
    }
    else
    {
        printf("Stack is not empty.\n");
    }
}
void size()
{
    printf("Stack size is %d.\n",top+1);
}
int main() {
    size();
    isempty();
    push(10);
    size();
    push(20);
    push(30);
    peak();
    size();
    pop();
    peak();
    pop();
    isempty();
    return 0;
}

//Queue
#include <stdio.h>
#define size 5
int top=-1;
int bottom=-1;
int array[size];
void enqueue(int value)
{
    if(bottom==size-1)
    {
        printf("Queue is full.\n");
    }
    else
    {
        if(top==-1)
        {
            top=0;
        }
        bottom++;
        array[bottom]=value;
        printf("%d is the value added in queue.\n",array[bottom]);
    }
}
void dequeue()
{
    if(top==-1||top>bottom)
    {
        printf("No data in Queue.\n");
    }
    else
    {
        printf("Deleted value: %d\n",array[top]);
        top++;
    }
}
void peak()
{
    if(top == -1||top>bottom)
    {
        printf("No data in Queue.\n");
    }
    else
    {
        printf("Peak value: %d\n",array[top]);
    }
}
void isempty()
{
    if(top == -1||top>bottom)
    {
        printf("No data in Queue.\n");
    }
    else
    {
        printf("Queue has value.\n");
    }
}
int main() {
    enqueue(10);
    enqueue(20);
    dequeue();
    peak();
    isempty();
    return 0;
}
