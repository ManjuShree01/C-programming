//Phone num
#include <stdio.h>
#include <stdlib.h>
struct cntct
{
    long int num;
    struct cntct *link;
};
int main()
{
    struct cntct *c1=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c2=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c3=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c4=(struct cntct*)malloc(sizeof(struct cntct));
    c1->num=9865326598;
    c2->num=6598326598;
    c3->num=6598743197;
    c4->num=8956431526;
    c1->link=c2;
    c2->link=c3;
    c3->link=c4;
    c4->link=NULL;
    struct cntct *head=c1;
    printf("Contact:\n");
    while(head!=NULL)
    {
        printf("%ld-->",head->num);
        head=head->link;
    }
    printf("NULL");
    return 0;
}
