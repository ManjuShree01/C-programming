//DLL
#include <stdio.h>
#include <stdlib.h>
struct cntct
{
    long int num;
    struct cntct *next;
    struct cntct *prev;
};
int main()
{
    struct cntct *c1=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c2=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c3=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c4=(struct cntct*)malloc(sizeof(struct cntct));
    c1->num=9865326598;
    c2->num=6598326598;
    c3->num=6598743197;
    c4->num=8956431526;
    //interconnection
   c1->prev=NULL;
   c1->next=c2;
   c2->prev=c1;
   c2->next=c3;
   c3->prev=c2;
   c3->next=c4;
   c4->prev=c3;
   c4->next=NULL;
    struct cntct *head=c1;
    printf("Contact:\n");
    while(head!=NULL)
    {
        printf("%ld-->",head->num);
        head=head->next;
    }
    printf("NULL");
    return 0;
}

//Music
#include <stdio.h>
#include <stdlib.h>
struct music
{
    int id;
    struct music *next;
    struct music *prev;
};
int main() 
{
    struct music *head=NULL,*temp;
    int m[3]={10,20,30};
    for(int i=0;i<3;i++)
    {
        struct music *newn=(struct music*)malloc(sizeof(struct music));
        newn->id=m[i];
        newn->next=head;
        newn->prev=NULL;
        if(head!=NULL)
        {
            head->prev=newn;
        }
        head=newn;
    }
    temp=head;
    while(temp!=NULL)
    {
        printf("%d",temp->id);
        temp=temp->next;
        if(temp!=NULL)
        {
            printf("<>");
        }
    }
    return 0;
}
