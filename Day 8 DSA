//Phone num
#include <stdio.h>
#include <stdlib.h>
struct cntct
{
    long int num;
    struct cntct *link;
};
int main()
{
    struct cntct *c1=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c2=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c3=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c4=(struct cntct*)malloc(sizeof(struct cntct));
    c1->num=9865326598;
    c2->num=6598326598;
    c3->num=6598743197;
    c4->num=8956431526;
    c1->link=c2;
    c2->link=c3;
    c3->link=c4;
    c4->link=NULL;
    struct cntct *head=c1;
    printf("Contact:\n");
    while(head!=NULL)
    {
        printf("%ld-->",head->num);
        head=head->link;
    }
    printf("NULL");
    return 0;
}

//Dialed no.
#include <stdio.h>
#include <stdlib.h>
struct cntct
{
    long int num;
    struct cntct *link;
};
int main()
{
    struct cntct *c1=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c2=(struct cntct*)malloc(sizeof(struct cntct));
    struct cntct *c3=(struct cntct*)malloc(sizeof(struct cntct));
    scanf("%d %d %d",&c1->num,&c2->num,&c3->num);
    c1->link=c2;
    c2->link=c3;
    c3->link=NULL;
    struct cntct *head=c1;
    while(head!=NULL)
    {
        printf("%ld-->",head->num);
        head=head->link;
    }
    printf("NULL");
    return 0;
}

//Roll no
#include <stdio.h>
#include <stdlib.h>
struct stud
{
    int roll;
    struct stud *link;
};
int main()
{
    struct stud *s1=(struct stud*)malloc(sizeof(struct stud));
    struct stud *s2=(struct stud*)malloc(sizeof(struct stud));
    scanf("%d %d",&s1->roll,&s2->roll);
    s1->link=s2;
    s2->link=NULL;
    struct stud *head=s1;
    while(head!=NULL)
    {
        printf("Roll %d -> ",head->roll);
        head=head->link;
    }
    printf("NULL");
    return 0;
}

//Cart
#include <stdio.h>
#include <stdlib.h>
struct cart
{
    int item;
    struct cart *link;
};
int main()
{
    struct cart *c1=(struct cart*)malloc(sizeof(struct cart));
    struct cart *c2=(struct cart*)malloc(sizeof(struct cart));
    struct cart *c3=(struct cart*)malloc(sizeof(struct cart));
    scanf("%d %d %d",&c1->item,&c2->item,&c3->item);
    c1->link=NULL;
    c2->link=c1;
    c3->link=c2;
    struct cart *head=c3;
    while(head!=NULL)
    {
        printf("%d -> ",head->item);
        head=head->link;
    }
    printf("NULL");
    return 0;
}

//Bank customers
#include <stdio.h>
#include <stdlib.h>
struct bank
{
    int acc;
    long int num;
    struct bank *link;
};
int main()
{
    struct bank *c[3];
    for(int i=0;i<3;i++)
    {
        c[i]=(struct bank*)malloc(sizeof(struct bank));
        printf("AccNo :",scanf("%d",&c[i]->acc),printf("Phone : "),scanf("%ld",&c[i]->num));
    }
    c[0]->link=c[1];
    c[1]->link=c[2];
    c[2]->link=NULL;
    struct bank *head=c[0];
    while(head!=NULL)
    {
        printf("%d - %ld -> ",head->acc,head->num);
        head=head->link;
    }
    printf("NULL");
    for(int i=0;i<3;i++)free(c[i]);
    return 0;
}

//Student
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
struct student {
    int roll;
    char name[20];
    char dept[20];
    struct student *link;
};
int main() {
    struct student *s[2];
    for(int i = 0; i < 2; i++) {
        s[i] = (struct student*)malloc(sizeof(struct student));
    }
    s[0]->roll = 1;
    strcpy(s[0]->name, "Arun");
    strcpy(s[0]->dept, "CSE");
    s[1]->roll = 2;
    strcpy(s[1]->name, "Meena");
    strcpy(s[1]->dept, "IT");
    s[0]->link = s[1];
    s[1]->link = NULL;
    struct student *head = s[0];
    while(head != NULL) {
        printf("%d %s %s", head->roll, head->name, head->dept);
        if(head->link != NULL) printf(" -> ");
        head = head->link;
    }
    printf(" -> NULL\n");
    for(int i = 0; i < 2; i++) {
        free(s[i]);
    }
    return 0;
}

//Order
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
struct order
{
    int id;
    char name[20];
    int price;
    char status[20];
    struct order *next;
};
int main()
{
    struct order *o[2];
    for(int i=0; i<2; i++)
    {
        o[i] = (struct order*)malloc(sizeof(struct order));
        printf("OrderID: ", scanf("%d", &o[i]->id));
        printf("Product: ", scanf("%s", o[i]->name));
        printf("Price: ", scanf("%d", &o[i]->price));
        printf("Status: ", scanf("%s", o[i]->status));
    }
    o[0]->next = o[1];
    o[1]->next = NULL;
    struct order *head = o[0];
    while(head != NULL)
    {
        printf("%d %s %d %s", head->id, head->name, head->price, head->status);
        if(head->next != NULL) printf(" -> ");
        head = head->next;
    }
    printf(" -> NULL");
    for(int i=0; i<2; i++) free(o[i]);
    return 0;
}
